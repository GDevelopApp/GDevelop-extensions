  "eventsFunctions": [
    {
      "description": "Add a force per second to an object in the 3`rd dimention to a 3d box object. This action creates the force using the specified angle (Z and X) and length.",
      "fullName": "Add a force to 3d box (per second)",
      "functionType": "Action",
      "group": "Instant force per second 3d",
      "name": "_3dBoxForcePerSecond",
      "sentence": "Add to _PARAM4_ an instant force, angle: _PARAM2_, angle X _PARAM3_ and length og _PARAM1_ pixels per second",
      "events": [
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [],
          "actions": [
            {
              "type": {
                "value": "MettreXY"
              },
              "parameters": [
                "object",
                "+",
                "(XFromAngleAndDistance((GetArgumentAsNumber(\"Angle\")), (sin((ToRad(GetArgumentAsNumber(\"Angletilt\")))))) * (GetArgumentAsNumber(\"Forcespeed\") * TimeDelta()))",
                "+",
                "(YFromAngleAndDistance((GetArgumentAsNumber(\"Angle\")), (sin(ToRad(GetArgumentAsNumber(\"Angletilt\"))))) * (GetArgumentAsNumber(\"Forcespeed\") * TimeDelta()))"
              ]
            },
            {
              "type": {
                "value": "Scene3D::Base3DBehavior::SetZ"
              },
              "parameters": [
                "object",
                "Object3D",
                "+",
                "(cos(ToRad(GetArgumentAsNumber(\"Angletilt\"))) * -(GetArgumentAsNumber(\"Forcespeed\") * TimeDelta()))"
              ]
            }
          ]
        }
      ],
      "parameters": [
        {
          "description": "Force speed in (in pixels per second)",
          "name": "ForceSpeed",
          "type": "expression"
        },
        {
          "description": "Angle Z (normal angle)",
          "name": "Angle",
          "type": "expression"
        },
        {
          "description": "Tilt angle (0 down, 180 up)",
          "name": "AngleTilt",
          "type": "expression"
        },
        {
          "description": "3dBox",
          "name": "Object",
          "supplementaryInformation": "Scene3D::Cube3DObject",
          "type": "objectList"
        }
      ],
      "objectGroups": [
        {
          "name": "object",
          "objects": [
            {
              "name": "Object"
            }
          ]
        }
      ]
    },
