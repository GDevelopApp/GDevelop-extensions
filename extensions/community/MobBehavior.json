  "eventsFunctions": [
    {
      "description": "Move the specified objects randomly.",
      "fullName": "Mob Movement",
      "functionType": "Action",
      "name": "RandomMovement",
      "sentence": "Move _PARAM1_  randomly with idle time _PARAM2_ with maximum range of _PARAM3_ on X axe and maximum range of _PARAM4_ on Y axe",
      "events": [
        {
          "infiniteLoopWarning": true,
          "type": "BuiltinCommonInstructions::While",
          "whileConditions": [
            {
              "type": {
                "value": "VarObjet"
              },
              "parameters": [
                "MovingObjects",
                "MovingObjectsMovement",
                "=",
                "0"
              ]
            },
            {
              "type": {
                "value": "VarObjet"
              },
              "parameters": [
                "MovingObjects",
                "MovingObjectsEating",
                "=",
                "0"
              ]
            },
            {
              "type": {
                "value": "VarGlobal"
              },
              "parameters": [
                "HungryTime",
                "=",
                "0"
              ]
            }
          ],
          "conditions": [],
          "actions": [
            {
              "type": {
                "value": "PathfindingBehavior::SetDestination"
              },
              "parameters": [
                "MovingObjects",
                "Pathfinding",
                "",
                "RandomFloatInRange(MovingObjects.X() - Distance_on_X_axe, MovingObjects.X() + Distance_on_X_axe)",
                "RandomFloatInRange(MovingObjects.Y() - Distance_on_Y_axe, MovingObjects.Y() + Distance_on_Y_axe)"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "MovingObjects",
                "MovingObjectsAngle",
                "=",
                "MovingObjects.Pathfinding::DestinationX()"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "MovingObjects",
                "MovingObjectsMovement",
                "=",
                "1"
              ]
            },
            {
              "type": {
                "value": "Wait"
              },
              "parameters": [
                "Duration"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "MovingObjects",
                "MovingObjectsMovement",
                "=",
                "0"
              ]
            }
          ]
        }
      ],
      "parameters": [
        {
          "description": "Make objects move randomly",
          "name": "MovingObjects",
          "supplementaryInformation": "Sprite",
          "type": "objectList"
        },
        {
          "description": "Idle time",
          "name": "Duration",
          "type": "expression"
        },
        {
          "description": "Maximum distance on X axe",
          "name": "Distance_on_X_axe",
          "type": "expression"
        },
        {
          "description": "Maximum distance on Y axe",
          "name": "Distance_on_Y_axe",
          "type": "expression"
        },
        {
          "description": "Needed for movement",
          "name": "Pathfinding",
          "supplementaryInformation": "PathfindingBehavior::PathfindingBehavior",
          "type": "behavior"
        }
      ],
      "objectGroups": []
    },
    {
      "description": "Gives a feeding system to the specified object making it eat.",
      "fullName": "Feeding system for your object",
      "functionType": "Action",
      "name": "FeedingSystem",
      "sentence": "The object _PARAM1_ will feed on object _PARAM3_ every _PARAM4_ seconds for _PARAM5_ seconds",
      "events": [
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [
            {
              "type": {
                "value": "DepartScene"
              },
              "parameters": [
                ""
              ]
            }
          ],
          "actions": [
            {
              "type": {
                "value": "ResetObjectTimer"
              },
              "parameters": [
                "MovingObjects",
                "\"Food\""
              ]
            }
          ]
        },
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [
            {
              "type": {
                "value": "CompareObjectTimer"
              },
              "parameters": [
                "MovingObjects",
                "\"Food\"",
                ">=",
                "HungryTime"
              ]
            }
          ],
          "actions": [
            {
              "type": {
                "value": "ModVarGlobal"
              },
              "parameters": [
                "HungryTime",
                "=",
                "1"
              ]
            }
          ]
        },
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [
            {
              "type": {
                "value": "CompareObjectTimer"
              },
              "parameters": [
                "MovingObjects",
                "\"Food\"",
                "<",
                "HungryTime"
              ]
            }
          ],
          "actions": [
            {
              "type": {
                "value": "ModVarGlobal"
              },
              "parameters": [
                "HungryTime",
                "=",
                "0"
              ]
            }
          ]
        },
        {
          "type": "BuiltinCommonInstructions::ForEach",
          "object": "MovingObjects",
          "conditions": [
            {
              "type": {
                "value": "VarGlobal"
              },
              "parameters": [
                "HungryTime",
                "=",
                "1"
              ]
            },
            {
              "type": {
                "value": "PickNearest"
              },
              "parameters": [
                "Food",
                "MovingObjects.X()",
                "MovingObjects.Y()",
                ""
              ]
            }
          ],
          "actions": [
            {
              "type": {
                "value": "PathfindingBehavior::SetDestination"
              },
              "parameters": [
                "MovingObjects",
                "Pathfinding",
                "",
                "Food.X()",
                "Food.Y()"
              ]
            },
            {
              "type": {
                "value": "ModVarObjet"
              },
              "parameters": [
                "MovingObjects",
                "MovingObjectsAngle",
                "=",
                "Food.X()"
              ]
            }
          ],
          "events": [
            {
              "type": "BuiltinCommonInstructions::Standard",
              "conditions": [
                {
                  "type": {
                    "value": "CollisionNP"
                  },
                  "parameters": [
                    "MovingObjects",
                    "Food",
                    "",
                    "",
                    ""
                  ]
                }
              ],
              "actions": [
                {
                  "type": {
                    "value": "ModVarObjet"
                  },
                  "parameters": [
                    "MovingObjects",
                    "MovingObjectsEating",
                    "=",
                    "1"
                  ]
                },
                {
                  "type": {
                    "value": "Wait"
                  },
                  "parameters": [
                    "FeedingTime"
                  ]
                },
                {
                  "type": {
                    "value": "ModVarObjet"
                  },
                  "parameters": [
                    "MovingObjects",
                    "MovingObjectsEating",
                    "=",
                    "0"
                  ]
                },
                {
                  "type": {
                    "value": "ResetObjectTimer"
                  },
                  "parameters": [
                    "MovingObjects",
                    "\"Food\""
                  ]
                }
              ]
            }
          ]
        }
      ],
      "parameters": [
        {
          "description": "Detect if object is hungry",
          "name": "MovingObjects",
          "supplementaryInformation": "Sprite",
          "type": "objectList"
        },
        {
          "description": "Needed to move",
          "name": "Pathfinding",
          "supplementaryInformation": "PathfindingBehavior::PathfindingBehavior",
          "type": "behavior"
        },
        {
          "description": "The food your object feeds on",
          "name": "Food",
          "supplementaryInformation": "Sprite",
          "type": "objectList"
        },
        {
          "description": "Duration in seconds of when to feed",
          "name": "HungryTime",
          "type": "expression"
        },
        {
          "description": "Duration in seconds of how long it takes the object to eat",
          "name": "FeedingTime",
          "type": "expression"
        }
      ],
