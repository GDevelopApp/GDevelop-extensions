    {
      "description": "Check if the theme is light.",
      "fullName": "Device theme is light",
      "functionType": "Condition",
      "name": "Light",
      "sentence": "Device theme is light",
      "events": [
        {
          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": [
            "const darkThemeQuery = window.matchMedia('(prefers-color-scheme: light)');\r",
            "\r",
            "if (darkThemeQuery.matches) {\r",
            "  eventsFunctionContext.returnValue = true;\r",
            "} else {\r",
            "  eventsFunctionContext.returnValue = false;\r",
            "}"
          ],
          "parameterObjects": "",
          "useStrict": true,
          "eventsSheetExpanded": true
        }
      ],
      "parameters": [],
      "objectGroups": []
    },
