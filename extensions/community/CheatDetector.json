  "eventsFunctions": [
    {
      "fullName": "",
      "functionType": "Action",
      "name": "onSceneLoaded",
      "sentence": "",
      "events": [
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [],
          "actions": [
            {
              "type": {
                "value": "JSONToVariableStructure"
              },
              "parameters": [
                "\"{}\"",
                "__CheatDetector_Stored"
              ]
            },
            {
              "type": {
                "value": "JSONToVariableStructure"
              },
              "parameters": [
                "\"{}\"",
                "__CheatDetector_Secured"
              ]
            },
            {
              "type": {
                "value": "SetSceneVariableAsBoolean"
              },
              "parameters": [
                "__CheatDetector_Cheated",
                "_Secured"
              ]
            },
            {
              "type": {
                "value": "ModVarScene"
              },
              "parameters": [
                "__CheatDetector_Verifier",
                "=",
                "RandomFloatInRange(1,2)"
              ]
            }
          ]
        }
      ],
      "parameters": [],
      "objectGroups": []
    },
    {
      "fullName": "",
      "functionType": "Action",
      "name": "onScenePreEvents",
      "sentence": "",
      "events": [
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [],
          "actions": []
        },
        {
          "type": "BuiltinCommonInstructions::ForEachChildVariable",
          "iterableVariableName": "__CheatDetector_Stored",
          "valueIteratorVariableName": "__CheatDetector_child",
          "keyIteratorVariableName": "__CheatDetector_index",
          "conditions": [],
          "actions": [],
          "events": [
            {
              "type": "BuiltinCommonInstructions::Standard",
              "conditions": [
                {
                  "type": {
                    "value": "BuiltinCommonInstructions::CompareNumbers"
                  },
                  "parameters": [
                    "round(Variable(__CheatDetector_Stored[VariableString(__CheatDetector_index)])/Variable(__CheatDetector_Verifier))",
                    "!=",
                    "Variable(__CheatDetector_Secured[VariableString(__CheatDetector_index)])"
                  ]
                }
              ],
              "actions": [
                {
                  "type": {
                    "value": "SetSceneVariableAsBoolean"
                  },
                  "parameters": [
                    "__CheatDetector_Cheated",
                    "True"
                  ]
                }
              ]
            }
          ]
        }
      ],
      "parameters": [],
      "objectGroups": []
    },
    {
      "description": "Did Player Cheated.",
      "fullName": "Did Player Cheated",
      "functionType": "Condition",
      "name": "HasCheated",
      "sentence": "Did Player Cheated?",
      "events": [
        {
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [
            {
              "type": {
                "value": "SceneVariableAsBoolean"
              },
              "parameters": [
                "__CheatDetector_Cheated",
                "True"
              ]
            }
          ],
          "actions": [
            {
              "type": {
                "value": "SetReturnBoolean"
              },
              "parameters": [
                "True"
              ]
            }
          ]
        }
      ],
      "parameters": [],
      "objectGroups": []
    },
    {
