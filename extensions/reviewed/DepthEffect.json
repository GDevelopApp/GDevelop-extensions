          "private": false,
          "sentence": "",
          "events": [
            {
              "disabled": false,
              "folded": false,
              "type": "BuiltinCommonInstructions::Comment",
              "color": {
                "b": 109,
                "g": 230,
                "r": 255,
                "textB": 0,
                "textG": 0,
                "textR": 0
              },
              "comment": "Determine the percent the object is from the horizon to the bottom of the screen.  \nMake sure this value never goes below 0 (otherwise it can become a positive value when squared).",
              "comment2": ""
            },
            {
              "disabled": false,
              "folded": false,
              "type": "BuiltinCommonInstructions::Standard",
              "conditions": [],
              "actions": [
                {
                  "type": {
                    "inverted": false,
                    "value": "DepthEffect::DepthEffect_Text::SetPropertyPercentAwayFromHorizon"
                  },
                  "parameters": [
                    "Object",
                    "Behavior",
                    "=",
                    "max(0,(Object.Y() - Object.Behavior::PropertyHorizonY()) / (SceneWindowHeight() -Object.Behavior::PropertyHorizonY()))"
                  ],
                  "subInstructions": []
                }
              ],
              "events": []
            },
            {
              "disabled": false,
              "folded": false,
              "type": "BuiltinCommonInstructions::Comment",
              "color": {
                "b": 109,
                "g": 230,
                "r": 255,
                "textB": 0,
                "textG": 0,
                "textR": 0
              },
              "comment": "Change the scale of the object based on it's relative position from horizon and bottom of screen.\nMake sure the exponent never goes below 1.",
              "comment2": ""
            },
            {
              "disabled": false,
              "folded": false,
              "type": "BuiltinCommonInstructions::Standard",
              "conditions": [],
              "actions": [
                {
                  "type": {
                    "inverted": false,
                    "value": "TextObject::Scale"
                  },
                  "parameters": [
                    "Object",
                    "=",
