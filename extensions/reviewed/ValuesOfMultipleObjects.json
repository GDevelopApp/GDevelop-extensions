    {
      "description": "Maximum X position of picked object instances (using AABB of objects).",
      "fullName": "Maximum X position of picked object instances",
      "functionType": "Expression",
      "name": "MaxX",
      "private": false,
      "sentence": "",
      "events": [
        {
          "disabled": false,
          "folded": false,
          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": "\n// If the instances doesn't exist we exit this function, returnValue will be 0\nif (objects.length == 0) return;\n\nlet minX = objects[0].getAABB().max[0] || 0;\n\nobjects.forEach((object) => {\n\n    let aabb = object.getAABB();\n    \n    if (aabb.max[0] > minX) {\n        minX = aabb.max[0];\n    }\n})\n\neventsFunctionContext.returnValue = minX;",
          "parameterObjects": "objects",
          "useStrict": true,
          "eventsSheetExpanded": true
        }
      ],
