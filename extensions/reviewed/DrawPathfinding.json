      "eventsFunctions": [
        {
          "description": "Draw the path followed by the object using a shape painter. It automatically creates an instance of the shape painter object if there is none.",
          "fullName": "Draw pathfinding",
          "functionType": "Action",
          "name": "DrawPathfinding",
          "sentence": "Draw the path followed by _PARAM0_ using _PARAM3_",
          "events": [
            {
              "type": "BuiltinCommonInstructions::Comment",
              "color": {
                "b": 109,
                "g": 230,
                "r": 255,
                "textB": 0,
                "textG": 0,
                "textR": 0
              },
              "comment": "Create the shape painter if one doesn't exist",
              "comment2": ""
            },
            {
              "type": "BuiltinCommonInstructions::Standard",
              "conditions": [
                {
                  "type": {
                    "value": "NbObjet"
                  },
                  "parameters": [
                    "ShapePainterObject",
                    "=",
                    "0"
                  ]
                }
              ],
              "actions": [
                {
                  "type": {
                    "value": "Create"
                  },
                  "parameters": [
                    "",
                    "ShapePainterObject",
                    "0",
                    "0",
                    "Object.Layer()"
                  ]
                },
                {
                  "type": {
                    "value": "ChangePlan"
                  },
                  "parameters": [
                    "ShapePainterObject",
                    "=",
                    "Object.ZOrder()-1"
                  ]
                }
              ]
            },
            {
              "type": "BuiltinCommonInstructions::Comment",
              "color": {
                "b": 109,
                "g": 230,
                "r": 255,
                "textB": 0,
                "textG": 0,
                "textR": 0
              },
              "comment": "Enforce absolute coordinates",
              "comment2": ""
            },
            {
              "type": "BuiltinCommonInstructions::Standard",
              "conditions": [
                {
                  "type": {
                    "value": "PrimitiveDrawing::AreCoordinatesRelative"
                  },
                  "parameters": [
                    "ShapePainterObject"
                  ]
                }
              ],
              "actions": [
                {
                  "type": {
                    "value": "PrimitiveDrawing::UseRelativeCoordinates"
                  },
                  "parameters": [
                    "ShapePainterObject",
                    "no"
                  ]
                }
              ]
            },
            {
              "type": "BuiltinCommonInstructions::Comment",
              "color": {
                "b": 109,
                "g": 230,
                "r": 255,
                "textB": 0,
                "textG": 0,
                "textR": 0
              },
              "comment": "Reset counter",
              "comment2": ""
            },
            {
              "type": "BuiltinCommonInstructions::Standard",
              "conditions": [],
              "actions": [
                {
                  "type": {
                    "value": "SetNumberVariable"
                  },
                  "parameters": [
                    "LoopIndex",
                    "=",
                    "0"
                  ]
                }
              ]
            },
            {
              "type": "BuiltinCommonInstructions::Comment",
              "color": {
                "b": 109,
                "g": 230,
                "r": 255,
                "textB": 0,
                "textG": 0,
                "textR": 0
              },
              "comment": "Draw path",
              "comment2": ""
            },
            {
              "type": "BuiltinCommonInstructions::Repeat",
              "repeatExpression": "Object.PathfindingBehavior::NodeCount()",
              "conditions": [],
              "actions": [],
              "events": [
                {
                  "type": "BuiltinCommonInstructions::Comment",
                  "color": {
                    "b": 109,
                    "g": 230,
                    "r": 255,
                    "textB": 0,
                    "textG": 0,
                    "textR": 0
                  },
                  "comment": "Draw circles on each node",
                  "comment2": ""
                },
                {
                  "type": "BuiltinCommonInstructions::Standard",
                  "conditions": [
                    {
                      "type": {
                        "value": "NumberVariable"
                      },
                      "parameters": [
                        "LoopIndex",
                        "<=",
                        "Object.PathfindingBehavior::NodeCount()-1"
                      ]
                    }
                  ],
                  "actions": [
                    {
                      "type": {
                        "value": "PrimitiveDrawing::Circle"
                      },
                      "parameters": [
                        "ShapePainterObject",
                        "Object.PathfindingBehavior::GetNodeX(LoopIndex)",
                        "Object.PathfindingBehavior::GetNodeY(LoopIndex)",
                        "3"
                      ]
                    }
                  ]
                },
                {
                  "type": "BuiltinCommonInstructions::Comment",
                  "color": {
                    "b": 109,
                    "g": 230,
                    "r": 255,
                    "textB": 0,
                    "textG": 0,
                    "textR": 0
                  },
                  "comment": "Draw lines between nodes",
                  "comment2": ""
                },
                {
                  "type": "BuiltinCommonInstructions::Standard",
                  "conditions": [
                    {
                      "type": {
                        "value": "NumberVariable"
                      },
                      "parameters": [
                        "LoopIndex",
                        "<",
                        "Object.PathfindingBehavior::NodeCount()-1"
                      ]
                    }
                  ],
                  "actions": [
                    {
                      "type": {
                        "value": "PrimitiveDrawing::LineV2"
                      },
                      "parameters": [
                        "ShapePainterObject",
                        "Object.PathfindingBehavior::GetNodeX(LoopIndex)",
                        "Object.PathfindingBehavior::GetNodeY(LoopIndex)",
                        "Object.PathfindingBehavior::GetNodeX(LoopIndex +1)",
                        "Object.PathfindingBehavior::GetNodeY(LoopIndex+1)",
                        "3"
                      ]
                    }
                  ]
                },
                {
                  "type": "BuiltinCommonInstructions::Standard",
                  "conditions": [],
                  "actions": [
                    {
                      "type": {
                        "value": "SetNumberVariable"
                      },
                      "parameters": [
                        "LoopIndex",
                        "+",
                        "1"
                      ]
                    }
                  ]
                }
              ]
            }
          ],
          "parameters": [
