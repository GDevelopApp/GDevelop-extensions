            "// Get all instances of \"object\"\r",
            "const lists = eventsFunctionContext.getObjectsLists(\"Object\").items;\r",
            "\r",
            "// Store the variable name\r",
            "const objectVariableName = eventsFunctionContext.getArgument(\"ObjectVariableName\");\r",
